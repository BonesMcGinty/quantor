{
  "title": "ðŸ§™ Quantor API Docs V2.1.2",
  "description": "Building a brighter future today for the kids of yesterday tomorrow.",
  "base": "https://mydocs.com",
  "endpoints": [
    {
      "name": "/api/user",
      "display": "User Data",
      "description": "Access all user data",
      "handlers": {
        "GET": {
          "responses": [
            {
              "code": 200,
              "model": "User"
            },
            {
              "code": 400,
              "model": "BadRequest"
            },
            {
              "code": 401,
              "model": "Unauthorized"
            }
          ],
          "requiredQueryParams": [
            {
              "name": "banner",
              "description": "Some explanation of what a banner is",
              "type": "String"
            },
            {
              "name": "uid",
              "description": "User Identification",
              "type": "Number"
            }
          ],
          "optionalQueryParams": [
            {
              "name": "lang",
              "description": "Some language [EN|FR]"
            }
          ],
          "requiredHeaders": [
            {
              "name": "jwt",
              "description": "A JSON Web Token",
              "type": "String"
            }
          ]
        },
        "POST": {
          "requiredBodyParams": [
            {
              "name": "uid"
            },
            {
              "name": "notifications"
            }
          ]
        },
        "PUT": {
          "requiredBodyParams": [
            {
              "name": "uid"
            },
            {
              "name": "notifications"
            }
          ]
        },
        "DELETE": {
          "requiredBodyParams": [
            {
              "name": "uid"
            },
            {
              "name": "notifications"
            }
          ]
        }
      }
    },
    {
      "name": "/api/people",
      "display": "People",
      "description": "Retrieve and modify all the peoples",
      "handlers": {
        "GET": {
          "requiredQueryParams": [
            {
              "name": "uid",
              "description": "Peoples ID"
            }
          ]
        },
        "POST": {
          "responses": [
            {
              "code": 418,
              "model": "Teapot"
            }
          ],
          "requiredBodyParams": [
            {
              "name": "uids",
              "description": "User IDs"
            },
            {
              "name": "people",
              "description": "Array of people objects"
            }
          ]
        }
      }
    },
    {
      "name": "/api/my-account",
      "display": "My Account",
      "description": "A user account",
      "handlers": {
        "GET": {
          "response": [
            {
              "code": 200,
              "model": "Product"
            }
          ],
          "requiredQueryParams": [
            {
              "name": "id",
              "description": "user id"
            },
            {
              "name": "language"
            },
            {
              "name": "year"
            },
            {
              "name": "somethingelse"
            }
          ]
        }
      }
    }
  ],
  "models": {
    "User": {
      "id": "String",
      "firstName": "String",
      "lastName": "String",
      "age": "Integer",
      "weight": "Float",
      "isSexy": "Boolean",
      "nested": {
        "yes": {
          "no": "Boolean",
          "maybe": "Doge"
        }
      }
    },
    "Product": {
      "id": "String",
      "name": "String",
      "description": "String",
      "display": "String",
      "price": "Float",
      "salePrice": "Float"
    },
    "BadRequest": {
      "msg": "String"
    },
    "Unauthorized": {
      "msg": "String"
    }
  }
}
